{
  "$schema": "https://json.schemastore.org/nest-cli",
  "language": "ts",
  "builder": "swc",
  "typeCheck": true,
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "tsconfig.json",
    "assets": [],
    "watchAssets": false
  },
  "projects": {
    "@oksai/api": {
      "type": "application",
      "root": ".",
      "entryFile": "main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    }
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node"
    },
    "esm": false
  }
}
